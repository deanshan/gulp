{
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0,
        "no-undef": 0,
        "no-mixed-spaces-and-tabs": [0, false],
        "no-empty": 0,
        "no-control-regex": 0,
        "no-cond-assign": 0,
        "no-extra-semi": 0
    }
}